#=============================================================================================
#=============================================================================================
#=============================================================================================
#this is where returning users will login 
#from here it will take them to the homepage
#the user will be required to enter there username/email and the correct password
#if the password and email don't match or they arn't in the database then a error message will occur
<LoginWindow>:
    canvas.before:
        Color:
            rgba: (1,1,1,1)
        Rectangle:
            pos: self.pos
            size: self.size
    name: "login"

    email: email
    password: password

    FloatLayout:

# logo at the top of the screen
        Image:
            source: 'graphics/Mglogo.png'
            size_hint: 0.8, 0.2
            pos_hint: {"x":0.08, "y":0.8}

#=============================================================================================
#label for entering the email
        Label:
            markup: True
            text: "[color=#454545][b]Email:[/b] [/color]"
            font_size: (15)
            pos_hint:{"x":0.055,"y":0.51}
            size_hint: 0.35, 0.15

#where the user enters there email
        TextInput:
            markup: True
            background_normal: ''
            background_color: (69/255, 69/255, 69/255, 1)
            foreground_color: (216/255, 216/255, 216/255, 1)
            id: email
            font_size: (12)
            multiline: False
            pos_hint:{"x":0.2,"y":0.5}
            size_hint: 0.6, 0.05
#=============================================================================================

#label for the password
        Label:
            markup: True
            text: "[color=#454545][b]Password:[/b] [/color]"
            font_size: (15)
            pos_hint:{"x":0.07,"y":0.4}
            size_hint: 0.35, 0.15


#where the usere enters there created password
        TextInput:
            markup: True
            background_normal: ''
            background_color: (69/255, 69/255, 69/255, 1)
            foreground_color: (216/255, 216/255, 216/255, 1)
            id: password
            font_size: (12)
            multiline: False
            password: True
            pos_hint:{"x":0.2,"y":0.4}
            size_hint: 0.6, 0.05
#=============================================================================================

#log in button
#if the email and password match then it takes you to the home page
        Button:

            markup: True
            background_normal: ''
            background_color: (126/255, 28/255, 19/255, 1)
            pos_hint:{"x":0.2,"y":0.3}
            size_hint: 0.6, 0.05
            text: "[color=#d8d8d8]Log in[/color]"
            font_size: (15)
            on_release:
                root.manager.transition.duration = 0
                root.loginBtn()
                #validates that the email and password match

#=============================================================================================

#labe; for user if they don't allready have an account
        Label:
            markup: True
            background_normal: ''
            background_color: (1, 1, 1, 1)
            pos_hint:{"x":0.2,"y":0.05}
            size_hint: 0.4, 0.1
            font_size: (15)
            text: "[color=#454545]Don't have an Account?[/color] "

#clicking this will tke the user to the create account page
        Button:
            markup: True
            background_normal: ''
            background_color: (0, 0, 0, 0)
            pos_hint:{"x":0.65,"y":0.05}
            size_hint: 0.15, 0.1
            font_size: (15)
            text: "[color=#7e1c13][b]Create Account[/b][/color]"
            on_release:
            
                root.manager.transition.direction = "right"
                root.manager.transition.duration = 0
                #makes the transition between screens seamless
                root.createBtn()



