#=============================================================================================
#this screen is for the user to create an account for teh application
#the user will be required to enter an username, email and a created password

<CreateAccountWindow>:
    canvas.before:
        Color:
            rgba: (1,1,1,1)
        Rectangle:
            pos: self.pos
            size: self.size
    name: "create"

    namee: namee
    email: email
    password: passw

    FloatLayout:
        cols:1

        FloatLayout:
            size: root.width, root.height/2
#=============================================================================================
            #logo at the top
            Image:
                source: 'graphics/Mglogo.png'
                size_hint: 0.8, 0.2
                pos_hint: {"x":0.08, "y":0.8}
                
            #label for the username
            Label:
                markup: True
                size_hint: 0.5,0.12
                pos_hint:{"x":0.005,"y":0.61}
                text: "[color=#454545][b]Username: [/b][/color]"
                font_size: (15)
            #where the user enters there preferd username
            TextInput:
                markup: True
                background_normal: ''
                background_color: (69/255, 69/255, 69/255, 1)
                foreground_color: (216/255, 216/255, 216/255, 1)
                pos_hint:{"x":0.2,"y":0.6}
                size_hint: 0.6, 0.05
                id: namee
                multiline: False
                font_size: (12)
#=============================================================================================


            #label for the email
            Label:
                markup: True
                size_hint: 0.5,0.12
                pos_hint:{"x":0.0006,"y":0.51}
                text: "[color=#454545][b]Email: [/b][/color]"
                font_size: (15)
            #where the user will enter there email
            TextInput:
                markup: True
                background_normal: ''
                background_color: (69/255, 69/255, 69/255, 1)
                foreground_color: (216/255, 216/255, 216/255, 1)
                pos_hint:{"x":0.2,"y":0.5}
                size_hint: 0.6, 0.05
                id: email
                multiline: False
                font_size: (12)
#=============================================================================================
            #the label for the passwword that the user creates
            Label:
                markup: True
                size_hint: 0.5,0.12
                pos_hint:{"x":0.005,"y":0.41}
                text: "[color=#454545][b]Password:[/b] [/color]"
                font_size: (15)
            #where the user creates there password
            TextInput:
                markup: True
                background_normal: ''
                background_color: (69/255, 69/255, 69/255, 1)
                foreground_color: (216/255, 216/255, 216/255, 1)
                pos_hint:{"x":0.2,"y":0.4}
                size_hint: 0.6, 0.05
                id: passw
                multiline: False
                password: True
                font_size: (12)
                color: (1,1,1,1)
#=============================================================================================
        #this is where the user will finaly create there account
        Button:
            markup: True
            background_normal: ''
            background_color: (126/255, 28/255, 19/255, 1)
            pos_hint:{"x":0.2,"y":0.3}
            size_hint: 0.6, 0.05
            text: "[color=#d8d8d8]Sign Up[/color]"
            font_size: (15)
            on_release:
            #takes the user to the log in screen
                app.root.current = "create"
                root.manager.transition.duration = 0
                root.submit()

#=============================================================================================
#label if the user alredy has an account
        Label:
            markup: True
            background_normal: ''
            background_color: (1, 1, 1, 1)
            pos_hint:{"x":0.2,"y":0.05}
            size_hint: 0.4, 0.1
            font_size: (15)
            text: "[color=#454545]Already have an Account?[/color]"


#if the user already has an account the click this and it  takes them to the log in screen
        Button:
            markup: True
            background_normal: ''
            background_color: (0, 0, 0, 0)
            pos_hint:{"x":0.7,"y":0.05}
            size_hint: 0.08, 0.1
            font_size: (15)
            text: "[color=#7e1c13][b]Log In[/b][/color]"
            on_release:
            #takes the user to the login page
                app.root.current = "login"
                root.manager.transition.direction = "left"
                #makes the transition seamless
                root.manager.transition.duration = 0
