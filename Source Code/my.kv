                center_y: self.parent.center_y
                #image used for the button






#=============================================================================================
#this is the logo at the bottom
        Image:
            source: 'graphics/Mglogo.png'
            size_hint: 0.8, 0.15
            pos_hint: {"x":0.08, "y":0.03}
#=============================================================================================
#this is the progress bar button 
        Image:
            source: 'graphics/pb1.png'
            pos_hint: {"x":0.18, "y":0.08}
            size_hint: 0.6, 0.2

#=============================================================================================
#this is the next button
#selecting this will ove the user on to the next page providing that they have filled al feilds

        Button:
            size_hint: 0.6, 0.08
            pos_hint:{"x":0.18, "y":0.19}
            background_color: 0,0,0,0



#this changes the color of the next button 
#this visably shows the user thaat the button has been selected
            on_press: root.next_on()
            on_release: root.next_off()


            on_release:
                # root.check() checks that all of the feids have been apropriatly filled
